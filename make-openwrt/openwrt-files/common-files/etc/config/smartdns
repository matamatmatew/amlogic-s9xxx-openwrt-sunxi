
config smartdns
	option enabled '1'
	option server_name 'smartdns'
	option port '53'
	option auto_set_dnsmasq '1'
	option tcp_server '1'
	option ipv6_server '1'
	option bind_device '1'
	option dualstack_ip_selection '1'
	option serve_expired '1'
	option cache_persist '1'
	option resolve_local_hostnames '1'
	option force_https_soa '1'
	option rr_ttl_min '600'
	option seconddns_port '6553'
	option seconddns_tcp_server '1'
	option seconddns_enabled '1'
	option enable_auto_update '1'
	option response_mode 'fastest-response'
	list conf_files 'neodevhost-smartdns.conf'
	list conf_files 'adrules-smartdns.conf'
	option old_port '53'
	option old_enabled '1'
	option old_auto_set_dnsmasq '1'

config domain-rule

config server
	option enabled '1'
	option name 'CF-IPv4'
	option type 'udp'
	option ip '1.1.1.1'
	option port '53'

config server
	option enabled '1'
	option name 'CF-IPv6'
	option ip '2606:4700:4700::1111'
	option type 'udp'
	option port '53'

config server
	option enabled '1'
	option name 'CF-DOH'
	option ip 'dns.cloudflare.com/dns-query'
	option type 'https'

config server
	option enabled '1'
	option name 'CF-DOT'
	option ip 'one.one.one.one'
	option type 'tls'
	option port '853'

config server
	option enabled '0'
	option name 'mull-IPv4'
	option ip '194.242.2.9'
	option type 'udp'

config server
	option enabled '0'
	option name 'mull-IPv6'
	option ip '2a07:e340::9	'
	option type 'udp'

config server
	option enabled '0'
	option name 'mull-DOH'
	option ip 'all.dns.mullvad.net/dns-query'
	option type 'https'

config server
	option enabled '0'
	option name 'mull-DOT'
	option ip 'all.dns.mullvad.net'
	option type 'tls'

config download-file
	option name 'neodevhost-smartdns.conf'
	option url 'https://neodev.team/smartdns.conf'
	option type 'config'

config download-file
	option name 'adrules-smartdns.conf'
	option url 'https://adrules.top/smart-dns.conf'
	option type 'config'

